
<div class="container-index m-5">

  <h1>List Of All Pokemons </h1>
    <div>
      <% @pokemons.each do |pokemon| %>
        <p><%= link_to pokemon.name , pokemon_path(pokemon)%></p>
      <% end %>
    </div>
  <p><%= link_to "Add New Pokemon", new_pokemon_path, class: "btn btn-primary" %></p>

</div>
